alias mv="mv -i"
alias rm="rm -i"
alias cp="cp -i"
alias exit="clear ; exit"
alias ping="ping -c3"
alias k="kubectl"
alias mk="minikube"
alias ktx="kubectx"
alias kns="kubens"
alias t="terraform"
alias g="gcloud"
alias a="aws"
